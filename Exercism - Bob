var Bob = function() {};

Bob.prototype.hey = function(input) {
var line = input.split('');
for (var i = 0; i<line.length;i++) {
  if (line[i] === '?') {
    return 'Sure.';
  } else if (line[i] === '!') {
    return 'Whoa, chill out!';

  } else if (line[i] === '.') {
    return 'Whatever.'
  }
}
if (input === null) {
  return 'Fine. Be that way!';
}
};
 //Bob.prototype.hey("what up?")
module.exports = Bob;
